# Kendi ortam değişkenlerinizi buraya kopyalayıp .env ismiyle oluşturun
# Örnek değerler — gerçek şifreleri buraya koymayın
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
PORT=3000
# Güçlü bir JWT secret belirleyin (prod ortamında güvenli şekilde saklayın)
JWT_SECRET=your_jwt_secret
